<div class="q-quiz-footer">
  <button id="q-next-question" class="q-quiz-button"><span class="q-lg">Next question</span><span class="q-sm">&gt;</span></button>
  <button id="q-quiz-results" class="q-quiz-button">Results</button>
  <div class="q-progress">
    <span id="q-number"><%= first %></span>
    of
    <span id="q-total"><%= total %></span>
  </div>
  <button id="q-previous-question" class="q-quiz-button"><span class="q-sm">&lt;</span><span class="q-lg">Previous question</span></button>
</div>
